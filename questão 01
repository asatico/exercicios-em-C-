using System;

namespace atividade c#
{
	class Program
	{
		public static void Main(string[] args)
		{
			int numero;
			
			
			Console.WriteLine("Escreva um núemro inteiro:");
			numero = Convert.ToInt32(Console.ReadLine());
			Console.WriteLine("O número que voce escrevel é:"+ numero);
			
			
			
			Console.Write("PRESSIONE ALGO PARA FECHAR . . . ");
			Console.ReadKey(true);
		}
	}
}
